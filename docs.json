{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "DialogShift Support Docs",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "index",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Essentials",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://dialogshift.mintlify.app/",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Blog",
          "href": "https://www.dialogshift.com/blog/blog-i",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@dialogshift.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "DialogShift App",
      "href": "https://app.dialogshift.com"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/dialogshift",
      "linkedin": "https://www.linkedin.com/company/dialogshift"
    }
  }
}